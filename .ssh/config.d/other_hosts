###################################################
# Other networks
###################################################

host ssh.dev.azure.com
    Hostname ssh.dev.azure.com
    IdentityFile ~/.ssh/keys/dev.azure.com/id_rsa
    User git
    AddKeysToAgent yes

host github.com
    UpdateHostKeys yes
    Hostname github.com
    User git
    AddKeysToAgent yes
    IdentityFile ~/.ssh/keys/github.com/id_ed25519

host gitlab.com
    IdentityFile ~/.ssh/keys/gitlab.com/id_ed25519
    IdentitiesOnly yes