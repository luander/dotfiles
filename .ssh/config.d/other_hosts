###################################################
# Other networks
###################################################

host ssh.dev.azure.com
    Hostname ssh.dev.azure.com
    IdentityFile ~/.ssh/id_rsa
    User git
    AddKeysToAgent yes

host github.com
    UpdateHostKeys yes
    Hostname github.com
    User git
    AddKeysToAgent yes
    IdentityFile ~/.ssh/keys/github.com/id_ed25519

